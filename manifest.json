{
    "manifest_version": 2,
    "name": "Temples CNPJ",
    "version": "1.0",
    "description": "This is a Chrome Extension to help with Temples IAN Payments in the BRZ Area",
    "icons": {
        "128": "temple-icon.png",
        "48": "temple-icon.png",
        "16": "temple-icon.png"
    },
    "browser_action": {
        "default_icon": "temple-icon.png"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ]
}
